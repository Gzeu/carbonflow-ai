{
  "name": "carbonflow-ai",
  "version": "2.0.0",
  "description": "Revolutionary AI-powered decentralized carbon credit trading platform with ML verification, blockchain automation, and enterprise-grade sustainability tracking - 2025 Edition",
  "main": "api/webhooks.js",
  "type": "module",
  "scripts": {
    "dev": "vercel dev --listen 3000",
    "build": "vercel build",
    "start": "vercel start",
    "deploy": "vercel --prod",
    "deploy-preview": "vercel",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint api --ext .js,.ts",
    "lint:fix": "eslint api --ext .js,.ts --fix",
    "format": "prettier --write api/**/*.{js,ts,json}",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level moderate",
    "setup-python": "python -m pip install -r requirements.txt",
    "test-ml": "python scripts/test_ml_models.py",
    "test-blockchain": "node scripts/test-blockchain.js",
    "validate-carbon": "node scripts/validate-carbon-credits.js"
  },
  "keywords": [
    "carbon-credits",
    "blockchain",
    "ai",
    "machine-learning",
    "sustainability",
    "github-app",
    "webhook",
    "carbon-tracking",
    "decentralized",
    "trading-platform",
    "environmental",
    "verification",
    "automation",
    "enterprise",
    "2025"
  ],
  "author": {
    "name": "George Pricop",
    "email": "pricopgeorge@gmail.com",
    "url": "https://github.com/Gzeu"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/carbonflow-ai.git"
  },
  "homepage": "https://carbonflow-ai.vercel.app",
  "dependencies": {
    "@vercel/node": "^3.2.17",
    "axios": "^1.7.7",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.14.2",
    "express": "^4.21.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^5.0.3",
    "web3": "^4.13.0",
    "ethers": "^6.13.4",
    "@tensorflow/tfjs-node": "^4.21.0",
    "ml-matrix": "^6.12.0",
    "simple-statistics": "^7.8.3",
    "natural": "^8.0.1",
    "uuid": "^10.0.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.2",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "vercel": {
    "functions": {
      "api/webhooks.js": {
        "maxDuration": 30
      }
    }
  }
}